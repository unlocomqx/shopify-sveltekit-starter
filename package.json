  "scripts": {
    "dev": "npm run db:push && kill-port --port 8081 && svelte-kit dev --port 8081",
    "dev:test": "dotenv -e .env.test -- npm run db:push && kill-port --port 8081 && TEST=1 svelte-kit dev --port 8081",
    "build": "svelte-kit build",
    "db:push": "prisma db push --accept-data-loss",
    "serve:test": "TEST=1 svelte-kit preview --port 8081 &",
    "preview:test": "TEST=1 npm run build && kill-port --port 8081 && npm run serve:test",
    "studio": "prisma studio",
    "studio:test": "dotenv -e .env.test prisma studio",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "jest": "jest ",
    "cypress:open": "cypress open | grep -v GET",
    "cypress:run": "cypress run --browser chrome",
    "e2e:open": "zx cypress/reset.mjs && run-p dev:test cypress:open",
    "e2e:run": "zx cypress/reset.mjs && run-s preview:test cypress:run"
